{
  "name": "tool",
  "description": "Tool Collection",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "tool"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . && stylelint \"**/*.{css,scss}\"",
    "format": "eslint --fix . && stylelint --fix \"**/*.{css,scss}\"",
    "commit": "git add . && git-cz",
    "prepare": "npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/cz-commitlint": "^17.7.1",
    "@iconify/json": "^2.2.102",
    "@nuxt/devtools": "^0.7.6",
    "@nuxt/image-edge": "1.0.0-28101255.bece998",
    "@nuxthq/ui": "^2.7.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/i18n": "8.0.0-rc.2",
    "@nuxtjs/stylelint-module": "^5.1.0",
    "@tiptap/extension-character-count": "^2.0.4",
    "@tiptap/extension-floating-menu": "^2.0.4",
    "@tiptap/extension-highlight": "^2.0.4",
    "@tiptap/extension-image": "^2.0.4",
    "@tiptap/extension-link": "^2.0.4",
    "@tiptap/extension-placeholder": "^2.0.4",
    "@tiptap/extension-subscript": "^2.0.4",
    "@tiptap/extension-superscript": "^2.0.4",
    "@tiptap/extension-table": "^2.0.4",
    "@tiptap/extension-table-cell": "^2.0.4",
    "@tiptap/extension-table-header": "^2.0.4",
    "@tiptap/extension-table-row": "^2.0.4",
    "@tiptap/extension-task-item": "^2.0.4",
    "@tiptap/extension-task-list": "^2.0.4",
    "@tiptap/extension-text-align": "^2.0.4",
    "@tiptap/extension-typography": "^2.0.4",
    "@tiptap/extension-underline": "^2.0.4",
    "@tiptap/pm": "^2.0.4",
    "@tiptap/starter-kit": "^2.0.4",
    "@tiptap/suggestion": "^2.0.4",
    "@tiptap/vue-3": "^2.0.4",
    "@types/node": "^18",
    "@vueuse/core": "^10.3.0",
    "@vueuse/nuxt": "^10.3.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.47.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "inquirer": "8",
    "lint-staged": "^13.3.0",
    "markdown-it": "^13.0.1",
    "nuxt": "^3.6.5",
    "nuxt-lodash": "^2.5.0",
    "sass": "^1.64.2",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "tippy.js": "^6.3.7",
    "tiptap-markdown": "^0.8.2",
    "typescript": "^5.1.6",
    "zod": "^3.22.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,vue,json,md}": "eslint --fix"
  }
}
